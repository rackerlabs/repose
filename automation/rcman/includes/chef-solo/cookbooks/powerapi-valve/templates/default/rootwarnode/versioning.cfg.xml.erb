<versioning xmlns="http://docs.rackspacecloud.com/repose/versioning/v2.0">
    <service-root href="<%= node[:fqdn]%>:8080"/>
  <version-mapping pp-dest-id="mocks" id="/v1" status="DEPRECATED">
    <media-types>
      <media-type type="application/vnd.vendor.service-v1+xml" base="application/xml"/>
      <media-type type="application/vnd.vendor.service+xml; version=1" base="application/xml"/>
      <media-type type="application/v1+xml" base="application/xml"/>
      <media-type type="application/vnd.rackspace; x=v1; y=xml" base="application/xml"/>
      <media-type type="application/vnd.rackspace; rnd=1; x=v1; rnd2=2; y=xml" base="application/xml"/>
      <media-type type="application/vnd.vendor.service-v1+json" base="application/json"/>
      <media-type type="application/vnd.vendor.service+json; version=1" base="application/json"/>
      <media-type type="application/v1+json" base="application/json"/>
      <media-type type="application/vnd.rackspace; x=v1; y=json" base="application/json"/>
      <media-type type="application/vnd.rackspace; rnd=1; x=v1; rnd2=2; y=json" base="application/json"/>
    </media-types>
  </version-mapping>
  <version-mapping pp-dest-id="service" id="/v2" status="CURRENT">
    <media-types>
      <media-type type="application/vnd.vendor.service-v2+xml" base="application/xml"/>
      <media-type type="application/vnd.vendor.service+xml; version=2" base="application/xml"/>
      <media-type type="application/v2+xml" base="application/xml"/>
      <media-type type="application/vnd.rackspace; x=v2; y=xml" base="application/xml"/>
      <media-type type="application/vnd.rackspace; rnd=1; x=v2; rnd2=2; y=xml" base="application/xml"/>
      <media-type type="application/vnd.vendor.service-v2+json" base="application/json"/>
      <media-type type="application/vnd.vendor.service+json; version=2" base="application/json"/>
      <media-type type="application/v2+json" base="application/json"/>
      <media-type type="application/vnd.rackspace; x=v2; y=json" base="application/json"/>
      <media-type type="application/vnd.rackspace; rnd=1; x=v2; rnd2=2; y=json" base="application/json"/>
    </media-types>
  </version-mapping>
  <version-mapping pp-dest-id="service" id="/v3/get3" status="CURRENT">
    <media-types>      
       <media-type type="application/v3+json" base="application/json" />
    </media-types>  
 </version-mapping>
</versioning>
