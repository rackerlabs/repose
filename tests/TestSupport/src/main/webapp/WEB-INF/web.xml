<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>mockTokenServlet</servlet-name>
        <servlet-class>com.testing.testsupport.mockTokenServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mockGetGroupsServlet</servlet-name>
        <servlet-class>com.testing.testsupport.mockGetGroupsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>version1</servlet-name>
        <servlet-class>com.testing.testsupport.version1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>version2</servlet-name>
        <servlet-class>com.testing.testsupport.version2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>limits</servlet-name>
        <servlet-class>com.testing.testsupport.limits</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mockTokenServlet</servlet-name>
        <url-pattern>/token/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mockGetGroupsServlet</servlet-name>
        <url-pattern>/users/cmarin1/groups/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>version2</servlet-name>
        <url-pattern>/version2/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>version1</servlet-name>
        <url-pattern>/version1/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>limits</servlet-name>
        <url-pattern>/limits/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
