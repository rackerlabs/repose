[Service]
{% if repose.systemd_opts.use_saxon %}
Environment="SAXON_HOME=/etc/repose"
{% endif %}
Environment="JAVA_OPTS={{ repose.systemd_opts.java_opts|join(' ') }} -Dcom.sun.management.jmxremote.port={{ jmxtrans.jmx.port }} -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dcom.sun.management.jmxremote.local.only=true"
