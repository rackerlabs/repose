<?xml version="1.0" encoding="UTF-8"?>

<power-proxy xmlns="http://docs.rackspacecloud.com/power-api/system-model/v1.0">
    <host id="proxy-n01" hostname="pp-n01.service.api.vendor.com" service-port="2101">
        <filters>
            <filter name="versioning" />
            <filter name="papi-datastore" />
            <filter name="rate-limiting" />
        </filters>
    </host>
    
    <host id="proxy-n02" hostname="pp-n02.service.api.vendor.com" service-port="2102">
        <filters>
            <filter name="versioning" />
            <filter name="papi-datastore" />
            <filter name="rate-limitng" />
        </filters>
    </host>
    
    <host id="service-v1" hostname="web-n01.service.api.vendor.com" href="http://service.api.vendor.com:8080/v1" />
    <host id="service-v2" hostname="web-n02.service.api.vendor.com" href="http://service.api.vendor.com:8080/v2" />
</power-proxy>